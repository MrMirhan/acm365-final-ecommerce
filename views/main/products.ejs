<!-- product_list start-->
<section class="product_list section_padding">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-12">
                <div class="section_tittle text-center">
                    <h2>awesome <span>shop</span></h2>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="product_list_slider owl-carousel">
                    <div class="single_product_list_slider">
                        <div class="row align-items-center justify-content-between">
                            <% for(const product of products){ %>
                                <div class="col-lg-3 col-sm-6">
                                    <div class="single_product_item">
                                        <a href="/product/<%= product.categoryRoute %>/<%= product.routeUrl %>"><img src="<%= product.mainImageUrl %>" alt=""></a>
                                        <div class="single_product_text">
                                            <h4>
                                                <a href="/product/<%= product.categoryRoute %>/<%= product.routeUrl %>">
                                                    <%= product.name %>
                                                </a>
                                            </h4>
                                            <h3>$
                                                <%= product.price %>
                                            </h3>
                                            <a href="/product/<%= product.categoryRoute %>/<%= product.routeUrl %>/addCart" class="add_cart">+ add to cart<i class="ti-heart"></i></a>
                                        </div>
                                    </div>
                                </div>
                                <% } %>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- product_list part start-->